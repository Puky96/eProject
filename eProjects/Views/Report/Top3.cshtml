@model IEnumerable<eProjects.Models.ProjectViewModels.Top3ListItem>

@{
    ViewData["Title"] = "Top3";
    Layout = "~/Views/Shared/_Layout_Report.cshtml";
}


<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.ProjectLeader)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Priority1)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Priority2)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Priority3)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProjectLeader)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Priority1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Priority2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Priority3)
            </td>
        </tr>
}
    </tbody>
</table>
