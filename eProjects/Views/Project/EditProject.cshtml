    {
        Layout = "~/Views/Shared/_Layout_Admin.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

}
<br />
<div class="row">
    <div class="col-md-8 col-md-offset-2">
        <form asp-action="EditProject">
