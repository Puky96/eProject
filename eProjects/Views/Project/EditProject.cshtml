                            <label asp-for="CM" class="control-label"></label>
                            <input asp-for="CM" class="form-control" />
                            <span asp-validation-for="CM" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="ETC" class="control-label"></label>
                            <input asp-for="ETC" class="form-control" />
                            <span asp-validation-for="ETC" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="ORA" class="control-label"></label>
                            <div class="row">
                                <div class="col-md-10">
                                    <input type="range" asp-for="ORA" class="form-control" value="@Model.ORA" min="0" max="100" step="0.1" oninput="showVal(this.value)" onchange="showVal(this.value)" />
                                </div>
                                <div class="col-md-2">
                                    <label id="textORA">@Model.ORA %</label>
                                </div>
                            </div>
                            <span asp-validation-for="ORA" class="text-danger"></span>
                        </div>
                        @Html.HiddenFor(model => model.FiscalYearOfCompletion)
                        <div class="form-group">
                            <div class="checkbox">
                                <label>
                                    <input asp-for="ActualSpending" /> @Html.DisplayNameFor(model => model.ActualSpending)
                                </label>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="checkbox">
