                                    {
                                        <div class="input-group date" data-provider="datepicker">
                                            <input asp-for="StartupEndDate" type="text" class="form-control" />
                                            <div class="input-group-addon glyphicon glyphicon-th"></div>
                                        </div>
                                    }
                                    else
                                    {
                                        var x = (Convert.ToDateTime(Model.StartupEndDate)).ToString("dd.MM.yyyy");
                                        <label>@x</label>

                                    }
