                                    <input asp-for="SuccesCriteriaMet" /> @Html.DisplayNameFor(model => model.SuccesCriteriaMet)
                                </label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label asp-for="Status" class="control-label"></label>
                            <select asp-for="Status" class="form-control" asp-items="@ViewBag.Status">
                                <option disabled>-----SELECT-----</option>
                            </select>
                            <span asp-validation-for="Status" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Comments" class="control-label"></label>
                            <textarea cols="50" rows="5" asp-for="Comments" class="form-control"></textarea>
                            <span asp-validation-for="Comments" class="text-danger"></span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <input type="submit" value="Edit Project" class="btn btn-block btn-primary" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/lib/bootstrap-datepicker/dist/js/bootstrap-datepicker.js"></script>
<script src="~/lib/bootstrap-sweetalert/dist/sweetalert.js"></script>

<script>
    function showVal(newVal) {
        $('#textORA').text(newVal + " %");
    }

    $('#FesabilityEndDate').datepicker({
        startDate: "@ViewBag.startDate",
        endDate: "@ViewBag.endDate"
    });
    $('#FesabilityActualEnd').datepicker({
        startDate: "@ViewBag.startDate"
    });
    $('#ConceptualEndDate').datepicker({
        startDate: "@ViewBag.startDate",
        endDate: "@ViewBag.endDate"
    });
    $('#ConceptualActualEnd').datepicker({
        startDate: "@ViewBag.startDate"
    });
    $('#DefinitionEndDate').datepicker({
        startDate: "@ViewBag.startDate",
        endDate: "@ViewBag.endDate"
    });
    $('#DefinitionActualEnd').datepicker({
        startDate: "@ViewBag.startDate"
    });
    $('#DesignConstructEndDate').datepicker({
        startDate: "@ViewBag.startDate",
        endDate: "@ViewBag.endDate"
    });
    $('#DesignConstructActualEnd').datepicker({
        startDate: "@ViewBag.startDate",
        endDate: "@ViewBag.endDate"
    });
    $('#StartupActualEnd').datepicker({
        startDate: "@ViewBag.startDate"
    });

    $(document).ready(function () {
        $('#PCIS').hide();
        $('#PT').hide();
        $('#EI').hide();
        $('#STRT').hide();
    })

    function PCISShow() {
        $('#PCIS').show();
    };

    function STRTShow() {
        $('#STRT').show();
    };

    function PTShow() {
        $('#PT').show();
    };

    function EIShow() {
        $('#EI').show();
    };
</script>
